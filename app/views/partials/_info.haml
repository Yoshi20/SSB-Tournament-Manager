.modal.fade#infoModal{tabindex: '-1', role: 'dialog', 'aria-hidden'=>'true'}
  .modal-dialog.modal-dialog-centered{role: 'document'}
    .modal-content
      .modal-header
        .modal-title
          %h3= 'Info'
        %button.close{type: 'button', 'data-dismiss'=>'modal', 'aria-label'=>'close'}
          %span{'aria-hidden'=>'true'}
            &times;
      .modal-body{style: 'margin-bottom: 0.325rem;'}
        = info_body
